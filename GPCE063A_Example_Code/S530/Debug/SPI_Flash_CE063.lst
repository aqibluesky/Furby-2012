0000973F                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SPI_Initial
                            	// Description : Initial SPI interface
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Initial: .proc
                            	F_SPI_Initial:
0000973F 90 D4              		push R1, R2 to [SP];
                            	
00009740 11 93 02 20        		R1 = [P_IOA_Dir];
00009742 09 B3 FF 7F        		R1 &= ~C_SPI_SI_Bit;
00009744 09 A3 00 70        		R1 |= C_SPI_DO_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00009746 19 D3 02 20        		[P_IOA_Dir] = R1;
                            		
00009748 11 93 03 20        		R1 = [P_IOA_Attrib];
0000974A 09 A3 00 F0        		R1 |= C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
0000974C 19 D3 03 20        		[P_IOA_Attrib] = R1;
                            		
0000974E 11 93 00 20        		R1 = [P_IOA_Data];
00009750 09 B3 FF 1F        		R1 &= ~(C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit );
00009752 09 A3 00 10        		R1 |= C_SPI_CS_Bit;
00009754 19 D3 00 20        		[P_IOA_Data] = R1;
                            	
                            	
00009756 11 93 06 20        		R1 = [P_IOB_Dir];			// IOB as Input Floating 
00009758 40 B2              		R1 &= 0x0000;
00009759 19 D3 06 20        		[P_IOB_Dir] = R1;
0000975B 11 93 07 20        		R1 = [P_IOB_Attrib];
0000975D 09 A3 FF FF        		R1 |= 0xFFFF;
0000975F 19 D3 07 20        		[P_IOB_Attrib] = R1;
00009761 11 93 04 20        		R1 = [P_IOB_Data];
00009763 40 B2              		R1 &= 0x0000;
00009764 19 D3 04 20        		[P_IOB_Data] = R1;
                            		
00009766 11 93 08 20        		R1 = [P_IO_Ctrl];
00009768 09 A3 00 04        		R1 |= C_SPI_CS_Hardware;
0000976A 19 D3 08 20        		[P_IO_Ctrl] = R1;
                            		
0000976C 09 93 00 08        		R1 = C_SPI_Reset;		// software reset SPI
0000976E 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            	
00009770 09 93 02 80        		R1 = C_SPI_Enable | C_MasterMode | C_SCKSEL_FPLL_Div_8;
00009772 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            		
00009774 09 93 00 80        		R1 = C_SPI_TX_INT_Flag | C_SPI_TX_INT_DIS | C_SPI_TX_FIFO_Level_0;
00009776 19 D3 61 20        		[P_SPI_TX_Status] = R1;
                            		
00009778 09 93 00 80        		R1 = C_SPI_RX_INT_Flag | C_SPI_RX_INT_DIS | C_SPI_RX_FIFO_Level_1;
0000977A 19 D3 63 20        		[P_SPI_RX_Status] = R1;
                            		
0000977C 11 93 50 20        		R1 = [P_INT_Ctrl];
0000977E 19 D3 50 20        		[P_INT_Ctrl] = R1;
00009780 40 92              		R1 = 0x0000;
00009781 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            		
00009783 90 90              		pop R1, R2 from [SP];
00009784 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadAWord
                            	// Description : Read a word data from flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High	
                            	// Return      : R1 = one word of data read
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_ReadAWord:	.proc
00009785 08 05 03 00        		R2 = SP + 3;
00009787 D2 92              		R1 = [R2++];
00009788 C2 94              		R2 = [R2]; 
                            		
                            	F_SPI_ReadAWord:
00009789 98 D8              		push r2, r4 to [sp];
0000978A 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            		
0000978C 43 98              		R4 = C_SPI_Flash_Read_CMD;
0000978D 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000978F 0A B5 FF 00        		R2 &= 0xFF;					// Address High
00009791 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00009793 79 99              		R4 = R1 lsr 4;
00009794 7C 99              		R4 = R4 lsr 4;				// Address Middle
00009795 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009797 09 B3 FF 00        		R1 &= 0xFF;					// Address Low
00009799 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
0000979B 09 93 55 00        		R1 = 0x55;					// Read One Word Data dummy clock
0000979D 19 D3 62 20        		[P_SPI_TX_Data] = R1;
0000979F 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
000097A1 11 93 65 20        		R1 = [P_SPI_Misc];	
000097A3 50 C2              		test R1, C_SPI_Busy_Flag;
000097A4 44 4E              		jnz ?L_Check_TXIF;
000097A5 19 D3 65 20        		[P_SPI_Misc] = R1;
                            		
                            	
000097A7 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
000097A9 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
000097AB 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
000097AD 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
000097AF 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Received Valid Data High from slave SPI device
000097B1 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Received Valid Data Low from slave SPI device
000097B3 59 93              		R1 = R1 LSL 4;
000097B4 59 93              		R1 = R1 LSL 4;
000097B5 02 A3              		R1 |= R2;					// Return one word data read from FLASH
                            	
000097B6 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
000097B8 98 92              		pop R2, R4 from [SP];
000097B9 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadNWords
                            	// Description : Get N words from external memory to buffer
                            	// Destory     : R1
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	_SPI_ReadNWords:      .proc
000097BA 08 09 03 00        	    R4 = SP + 3;
000097BC D4 92              	    R1 = [R4++];							// buffer address
000097BD D4 94              	    R2 = [R4++];							// data length
000097BE D4 96              	    R3 = [R4++];							// external memory address low byte
000097BF C4 98              	    R4 = [R4];							// external memory address high byte
                            	
                            	F_SPI_ReadNWords:
000097C0 A8 DA              		push R1, R5 to [SP];
000097C1 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            		
000097C3 43 9A              		R5 = C_SPI_Flash_Read_CMD;
000097C4 1D DB 62 20        		[P_SPI_TX_Data] = R5;
000097C6 0C B9 FF 00        		R4 &= 0xFF;					// Address High
000097C8 1C D9 62 20        		[P_SPI_TX_Data] = R4;
000097CA 7B 99              		R4 = R3 lsr 4;
000097CB 7C 99              		R4 = R4 lsr 4;				// Address Middle
000097CC 1C D9 62 20        		[P_SPI_TX_Data] = R4;
000097CE 0B B7 FF 00        		R3 &= 0xFF;					// Address Low
000097D0 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
000097D2 15 9B 65 20        		R5 = [P_SPI_Misc];	
000097D4 50 CA              		test R5, C_SPI_Busy_Flag;
000097D5 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
000097D6 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
000097D8 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
000097DA 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
000097DC 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
                            	
                            	?L_GetNWordLoop:
000097DE 1B D7 62 20        		[P_SPI_TX_Data] = R3;		// Read One Word Data dummy clock
000097E0 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            	
                            	?L_GetNWordLoop_Check_TXIF:		// Wait untill command and address have been sent out
000097E2 15 9B 65 20        		R5 = [P_SPI_Misc];	
000097E4 50 CA              		test R5, C_SPI_Busy_Flag;
000097E5 44 4E              		jnz ?L_GetNWordLoop_Check_TXIF;
                            	
                            		
000097E6 13 97 64 20        		R3 = [P_SPI_RX_Data];		// low byte data
000097E8 14 99 64 20        		R4 = [P_SPI_RX_Data];		// high byte data
000097EA 5C 99              		R4 = R4 lsl 4;
000097EB 5C 99              		R4 = R4 lsl 4;
000097EC 03 A9              		R4 |= R3;					// Return one word data read from FLASH
000097ED D1 D8              		[R1++] = R4;
000097EE 41 24              		R2 -= 1;
000097EF 52 4E              		jnz ?L_GetNWordLoop;
                            	
000097F0 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            		
                            	?_Finish_Last:
000097F2 A8 90              		pop R1, R5 from [SP];
                            	
000097F3 90 9A              		retf;
                            	.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendAWord
                            	// Description : Write a word data to flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High, R3 = one word of data
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_SendAWord:	.proc
000097F4 08 07 03 00        		R3 = SP + 3;
000097F6 D3 92              		R1 = [R3++];
000097F7 D3 94              		R2 = [R3++];
000097F8 C3 96              		R3 = [R3];
                            		
                            	F_SPI_SendAWord:
000097F9 A8 DA              		push R1, R5 to [SP];
                            		
000097FA 40 F0 79 98        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
000097FC 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            	
000097FE 42 98              		R4 = C_SPI_Flash_Page_Program;
000097FF 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009801 0A B5 FF 00        		R2 &= 0xFF;						// Address High
00009803 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00009805 79 99              		R4 = R1 lsr 4;
00009806 7C 99              		R4 = R4 lsr 4;					// Address Middle
00009807 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009809 09 B3 FF 00        		R1 &= 0xFF;						// Address Low
0000980B 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
0000980D 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data Low Byte
0000980F 7B 97              		R3 = R3 lsr 4;
00009810 7B 97              		R3 = R3 lsr 4;
00009811 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data High Byte
                            		
                            	
                            	?L_Check_TXIF:
00009813 11 93 65 20        		R1 = [P_SPI_Misc];	
00009815 50 C2              		test R1, C_SPI_Busy_Flag;
00009816 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00009817 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            	
00009819 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000981B 12 95 64 20        		R2 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000981D 13 97 64 20        		R3 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000981F 14 99 64 20        		R4 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009821 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009823 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            		
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
00009825 40 F0 9C 98        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
00009827 41 C2              		test R1, C_Flash_Busy;
00009828 44 4E              		jnz ?L_Check_Busy;
                            		
00009829 A8 90              		pop R1, R5 from [SP];
0000982A 90 9A              		retf;	
                            		.endp
                            	
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendNWords
                            	// Description : Send N words to external memory from internal buffer
                            	// Destory     : None
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
