0000982E D4 94              		R2 = [R4++];
0000982F D4 96              		R3 = [R4++];
00009830 C4 98              		R4 = [R4];
                            	F_SPI_SendNWords:
00009831 A8 DA              		push R1, R5 to [SP];
                            	
                            	?L_WriteData:
                            		
00009832 40 F0 79 98        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
00009834 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            	
00009836 42 9A              		R5 = C_SPI_Flash_Page_Program;
00009837 1D DB 62 20        		[P_SPI_TX_Data] = R5;
00009839 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000983B 7B 9B              		R5 = R3 lsr 4;
0000983C 7D 9B              		R5 = R5 lsr 4;					// Address Middle
0000983D 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000983F 0B BB FF 00        		R5 = R3 & 0xFF;					// Address Low
00009841 1D DB 62 20        		[P_SPI_TX_Data] = R5;
                            		
                            	
                            	?L_Check_TXIF:
00009843 15 9B 65 20        		R5 = [P_SPI_Misc];	
00009845 50 CA              		test R5, C_SPI_Busy_Flag;
00009846 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00009847 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009849 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000984B 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000984D 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            	
                            	?L_SendDataLoop:
0000984F D1 9A              		R5 = [R1++];
00009850 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data Low Byte
00009852 7D 9B              		R5 = R5 lsr 4;
00009853 7D 9B              		R5 = R5 lsr 4;
00009854 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data High Byte
                            	
                            	?L_Check_TXIF_2:
00009856 15 9B 65 20        		R5 = [P_SPI_Misc];	
00009858 50 CA              		test R5, C_SPI_Busy_Flag;
00009859 44 4E              		jnz ?L_Check_TXIF_2;
                            	
0000985A 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000985C 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO
0000985E 41 24              		R2 -= 1;
0000985F 11 5E              		jz ?L_SendDataEnd;
00009860 42 06              		R3 += 2;
00009861 0B C7 FF 00        		test R3, 0x00FF;
00009863 55 4E              		jnz ?L_SendDataLoop;
                            	?L_JumpToNextPage:
00009864 40 46              		cmp R3, 0x0000;
00009865 01 4E              		jne ?L_WriteCurrentPage;
00009866 41 08              		R4 += 1;
