0000986C 41 C2              		test R1, C_Flash_Busy;
0000986D 44 4E              		jnz ?L_Check_Busy_1;
0000986E 88 90              		pop R1 from [SP];
0000986F 0F 9F 32 98        		pc = ?L_WriteData;
                            	
                            	?L_SendDataEnd:
00009871 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            	
                            	
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
00009873 40 F0 9C 98        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
00009875 41 C2              		test R1, C_Flash_Busy;
00009876 44 4E              		jnz ?L_Check_Busy;
                            		
                            	
00009877 A8 90              		pop R1, R5 from [SP];
00009878 90 9A              		retf;
                            		.endp
                            	
