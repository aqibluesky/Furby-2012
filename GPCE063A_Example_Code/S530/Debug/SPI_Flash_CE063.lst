000098EC 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            	
                            	
                            	?L_Check_TXIF:
000098EE 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
000098F0 50 C2              		test R1, C_SPI_Busy_Flag;
000098F1 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
000098F2 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            	
000098F4 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
000098F6 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
000098F8 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
000098FA 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
                            		
                            	?L_Check_Busy:					// Wait untill sector has been erased successfully (about 1 to 3 seconds)
