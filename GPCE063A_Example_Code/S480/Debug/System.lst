                            	//****************************************************************
                            	// Function    : F_Key_DebounceCnt_Down
                            	// Description : Debounce counter countdown
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	F_Key_DebounceCnt_Down:
00008F4F 11 93 E8 01        		R1 = [R_DebounceCnt];
00008F51 03 5E              		jz	L_DebounceCntZero;
00008F52 41 22              		R1 -= 0x0001;
00008F53 19 D3 E8 01        		[R_DebounceCnt] = R1;
                            	L_DebounceCntZero:
00008F55 90 9A              		retf;
                            		
                            	//****************************************************************
                            	// Function    : F_Key_Scan_ServiceLoop
                            	// Description : Get Key code from key pad(8 x 1 key pad)
                            	// Destory     : R1, R2
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : C_SCAN_8_Bits  = 1 , ScanKey 8bits
                            	//               C_SCAN_8_Bits  = 0 , ScanKey 16bits  
                            	//****************************************************************
                            	F_Key_Scan_ServiceLoop:	.proc
                            	
00008F56 11 93 00 20        		R1 = [P_IOA_Data];				// get key data from IOA  
                            	.IF C_SCAN_8_Bits
                            		//////////////////// Avoid Latch PortA Data Error 
00008F58 09 B3 00 FF        		R1 &= 0xFF00;  			          //R1 &= 0xFFFF;
00008F5A 19 D3 00 20        		[P_IOA_Data] = R1;
00008F5C 11 93 00 20        		R1 = [P_IOA_Data];
                            		//////////////////// 
00008F5E 09 B3 FF 00        		R1 &= 0x00FF; 			          
                            	.ELSE	
                            		R1 &= 0xFFFF; 
                            	.ENDIF
                            	
00008F60 12 95 E7 01        		R2 = [R_DebounceReg];
00008F62 19 D3 E7 01        		[R_DebounceReg] = R1;
00008F64 12 45 E7 01        		cmp R2, [R_DebounceReg];
00008F66 04 5E              		je ?L_KS_StableTwoSample;
                            	
00008F67 48 92              		R1 = C_DebounceCnt;				   // debounce counter reset
00008F68 19 D3 E8 01        		[R_DebounceCnt] = R1;
00008F6A 90 9A              		retf;
                            	
                            	?L_KS_StableTwoSample:
00008F6B 11 93 E8 01        		R1 = [R_DebounceCnt];
00008F6D 01 5E              		jz ?L_KS_StableOverDebounce;
00008F6E 90 9A              		retf;
                            	
                            	?L_KS_StableOverDebounce:
00008F6F 19 D3 E8 01        		[R_DebounceCnt] = R1;
00008F71 12 95 E7 01        		R2 = [R_DebounceReg];
00008F73 11 93 E9 01        		R1 = [R_KeyBuf];
00008F75 1A D5 E9 01        		[R_KeyBuf] = R2;
                            	
                            	.IF  C_SCAN_8_Bits	
00008F77 09 83 FF 00        		R1 ^= 0x00FF;    					
