                            	//****************************************************************
                            	// Function    : F_SP_SwitchChannel
                            	// Description : Switch A/D channel
                            	// Destory     : R1, R2
                            	// Parameter   : R1 = A/D channel
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SP_SwitchChannel: .proc
00008F0A 08 03 03 00        		R1 = SP + 3;
00008F0C C1 92              		R1 = [R1];
                            	F_SP_SwitchChannel:
00008F0D 40 42              		cmp R1, 0;
00008F0E 0A 4E              		jnz ?L_LineIn;
00008F0F 09 93 00 C0        		R1 = C_ADC_Auto_Scan_Busy | C_ADC_Busy;
                            	?L_Wait_AD_Ready_MIC:
00008F11 11 C3 70 20        		test R1, [P_ADC_Ctrl];
00008F13 43 4E              		jnz ?L_Wait_AD_Ready_MIC;
                            	
00008F14 09 93 EC 10        		R1 = C_ADC_Enable | C_AGC_Enable | C_ADC_CLK_FPLL_Div_32 | C_ADC_Bias_Enable | C_ADC_MIC_Enable | C_ADC_Timer_A
00008F16 19 D3 70 20        		[P_ADC_Ctrl] = R1;
                            	
00008F18 0C EE              		jmp ?L_SwitchChannelEnd;
                            		
                            	?L_LineIn:
00008F19 09 93 00 C0        		R1 = C_ADC_Auto_Scan_Busy | C_ADC_Busy;
                            	?L_Wait_AD_Ready_LinIn:
00008F1B 11 C3 70 20        		test R1, [P_ADC_Ctrl];
00008F1D 43 4E              		jnz ?L_Wait_AD_Ready_LinIn;
                            		
00008F1E 41 92              		R1 = C_ADC_AN0_Enable
00008F1F 19 D3 72 20        		[P_ADC_LineIn_BitCtrl] = R1
                            		
                            		
00008F21 09 93 D4 10        		R1 =C_ADC_Bias_Enable | C_ADC_CLK_FPLL_Div_32 | C_ADC_Timer_A | C_ADC_Manual_AN0 | C_ADC_Enable | C_ADC_Ch0_in_LineinPB0;
00008F23 19 D3 70 20        		[P_ADC_Ctrl] = R1;
                            	
                            	?L_SwitchChannelEnd:
00008F25 90 9A              		retf;
                            		.endp
                            	
                            	
