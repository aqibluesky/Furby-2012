                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_Flash_Write_Enable:	.proc
                            	F_Flash_Write_Enable:
000090E0 88 D2              		push r1 to [sp];
                            	
000090E1 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
000090E3 46 92              		R1 = C_SPI_Flash_Write_Enable;
000090E4 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            	
                            	
                            	?L_Check_TXIF:
000090E6 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
000090E8 50 C2              		test R1, C_SPI_Busy_Flag;
000090E9 44 4E              		jnz ?L_Check_TXIF;
                            	
                            	
000090EA 65 F1              		nop;
000090EB 65 F1              		nop;
000090EC 65 F1              		nop;
000090ED 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
                            		
000090EF 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
000090F1 88 90              		pop r1 from [sp];
000090F2 90 9A              		retf;
                            		.endp
                            	
