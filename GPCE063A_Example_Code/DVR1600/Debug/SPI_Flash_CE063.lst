00008FA6                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SPI_Initial
                            	// Description : Initial SPI interface
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Initial: .proc
                            	F_SPI_Initial:
00008FA6 90 D4              		push R1, R2 to [SP];
                            	
00008FA7 11 93 02 20        		R1 = [P_IOA_Dir];
00008FA9 09 B3 FF 7F        		R1 &= ~C_SPI_SI_Bit;
00008FAB 09 A3 00 70        		R1 |= C_SPI_DO_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00008FAD 19 D3 02 20        		[P_IOA_Dir] = R1;
                            		
00008FAF 11 93 03 20        		R1 = [P_IOA_Attrib];
00008FB1 09 A3 00 F0        		R1 |= C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00008FB3 19 D3 03 20        		[P_IOA_Attrib] = R1;
                            		
00008FB5 11 93 00 20        		R1 = [P_IOA_Data];
00008FB7 09 B3 FF 1F        		R1 &= ~(C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit );
00008FB9 09 A3 00 10        		R1 |= C_SPI_CS_Bit;
00008FBB 19 D3 00 20        		[P_IOA_Data] = R1;
                            	
                            	
00008FBD 11 93 06 20        		R1 = [P_IOB_Dir];			// IOB as Input Floating 
00008FBF 40 B2              		R1 &= 0x0000;
00008FC0 19 D3 06 20        		[P_IOB_Dir] = R1;
00008FC2 11 93 07 20        		R1 = [P_IOB_Attrib];
00008FC4 09 A3 FF FF        		R1 |= 0xFFFF;
00008FC6 19 D3 07 20        		[P_IOB_Attrib] = R1;
00008FC8 11 93 04 20        		R1 = [P_IOB_Data];
00008FCA 40 B2              		R1 &= 0x0000;
00008FCB 19 D3 04 20        		[P_IOB_Data] = R1;
                            		
00008FCD 11 93 08 20        		R1 = [P_IO_Ctrl];
00008FCF 09 A3 00 04        		R1 |= C_SPI_CS_Hardware;
00008FD1 19 D3 08 20        		[P_IO_Ctrl] = R1;
                            		
00008FD3 09 93 00 08        		R1 = C_SPI_Reset;		// software reset SPI
00008FD5 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            	
00008FD7 09 93 02 80        		R1 = C_SPI_Enable | C_MasterMode | C_SCKSEL_FPLL_Div_8;
00008FD9 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            		
00008FDB 09 93 00 80        		R1 = C_SPI_TX_INT_Flag | C_SPI_TX_INT_DIS | C_SPI_TX_FIFO_Level_0;
00008FDD 19 D3 61 20        		[P_SPI_TX_Status] = R1;
                            		
00008FDF 09 93 00 80        		R1 = C_SPI_RX_INT_Flag | C_SPI_RX_INT_DIS | C_SPI_RX_FIFO_Level_1;
00008FE1 19 D3 63 20        		[P_SPI_RX_Status] = R1;
                            		
00008FE3 11 93 50 20        		R1 = [P_INT_Ctrl];
00008FE5 19 D3 50 20        		[P_INT_Ctrl] = R1;
00008FE7 40 92              		R1 = 0x0000;
00008FE8 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            		
00008FEA 90 90              		pop R1, R2 from [SP];
00008FEB 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadAWord
                            	// Description : Read a word data from flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High	
                            	// Return      : R1 = one word of data read
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_ReadAWord:	.proc
00008FEC 08 05 03 00        		R2 = SP + 3;
00008FEE D2 92              		R1 = [R2++];
00008FEF C2 94              		R2 = [R2]; 
                            		
                            	F_SPI_ReadAWord:
00008FF0 98 D8              		push r2, r4 to [sp];
00008FF1 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            		
00008FF3 43 98              		R4 = C_SPI_Flash_Read_CMD;
00008FF4 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008FF6 0A B5 FF 00        		R2 &= 0xFF;					// Address High
00008FF8 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00008FFA 79 99              		R4 = R1 lsr 4;
00008FFB 7C 99              		R4 = R4 lsr 4;				// Address Middle
00008FFC 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008FFE 09 B3 FF 00        		R1 &= 0xFF;					// Address Low
00009000 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
00009002 09 93 55 00        		R1 = 0x55;					// Read One Word Data dummy clock
00009004 19 D3 62 20        		[P_SPI_TX_Data] = R1;
00009006 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00009008 11 93 65 20        		R1 = [P_SPI_Misc];	
0000900A 50 C2              		test R1, C_SPI_Busy_Flag;
0000900B 44 4E              		jnz ?L_Check_TXIF;
0000900C 19 D3 65 20        		[P_SPI_Misc] = R1;
                            		
                            	
0000900E 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
00009010 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00009012 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00009014 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
00009016 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Received Valid Data High from slave SPI device
00009018 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Received Valid Data Low from slave SPI device
0000901A 59 93              		R1 = R1 LSL 4;
0000901B 59 93              		R1 = R1 LSL 4;
0000901C 02 A3              		R1 |= R2;					// Return one word data read from FLASH
                            	
0000901D 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
0000901F 98 92              		pop R2, R4 from [SP];
00009020 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadNWords
                            	// Description : Get N words from external memory to buffer
                            	// Destory     : R1
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	_SPI_ReadNWords:      .proc
00009021 08 09 03 00        	    R4 = SP + 3;
00009023 D4 92              	    R1 = [R4++];							// buffer address
00009024 D4 94              	    R2 = [R4++];							// data length
00009025 D4 96              	    R3 = [R4++];							// external memory address low byte
00009026 C4 98              	    R4 = [R4];							// external memory address high byte
                            	
                            	F_SPI_ReadNWords:
00009027 A8 DA              		push R1, R5 to [SP];
00009028 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            		
0000902A 43 9A              		R5 = C_SPI_Flash_Read_CMD;
0000902B 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000902D 0C B9 FF 00        		R4 &= 0xFF;					// Address High
0000902F 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009031 7B 99              		R4 = R3 lsr 4;
00009032 7C 99              		R4 = R4 lsr 4;				// Address Middle
00009033 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009035 0B B7 FF 00        		R3 &= 0xFF;					// Address Low
00009037 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00009039 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000903B 50 CA              		test R5, C_SPI_Busy_Flag;
0000903C 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000903D 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
0000903F 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00009041 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00009043 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
                            	
                            	?L_GetNWordLoop:
00009045 1B D7 62 20        		[P_SPI_TX_Data] = R3;		// Read One Word Data dummy clock
00009047 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            	
                            	?L_GetNWordLoop_Check_TXIF:		// Wait untill command and address have been sent out
00009049 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000904B 50 CA              		test R5, C_SPI_Busy_Flag;
0000904C 44 4E              		jnz ?L_GetNWordLoop_Check_TXIF;
                            	
                            		
0000904D 13 97 64 20        		R3 = [P_SPI_RX_Data];		// low byte data
0000904F 14 99 64 20        		R4 = [P_SPI_RX_Data];		// high byte data
00009051 5C 99              		R4 = R4 lsl 4;
00009052 5C 99              		R4 = R4 lsl 4;
00009053 03 A9              		R4 |= R3;					// Return one word data read from FLASH
00009054 D1 D8              		[R1++] = R4;
00009055 41 24              		R2 -= 1;
00009056 52 4E              		jnz ?L_GetNWordLoop;
                            	
00009057 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            		
                            	?_Finish_Last:
00009059 A8 90              		pop R1, R5 from [SP];
                            	
0000905A 90 9A              		retf;
                            	.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendAWord
                            	// Description : Write a word data to flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High, R3 = one word of data
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_SendAWord:	.proc
0000905B 08 07 03 00        		R3 = SP + 3;
0000905D D3 92              		R1 = [R3++];
0000905E D3 94              		R2 = [R3++];
0000905F C3 96              		R3 = [R3];
                            		
                            	F_SPI_SendAWord:
00009060 A8 DA              		push R1, R5 to [SP];
                            		
00009061 40 F0 E0 90        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
00009063 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            	
00009065 42 98              		R4 = C_SPI_Flash_Page_Program;
00009066 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009068 0A B5 FF 00        		R2 &= 0xFF;						// Address High
0000906A 1A D5 62 20        		[P_SPI_TX_Data] = R2;
0000906C 79 99              		R4 = R1 lsr 4;
0000906D 7C 99              		R4 = R4 lsr 4;					// Address Middle
0000906E 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009070 09 B3 FF 00        		R1 &= 0xFF;						// Address Low
00009072 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
00009074 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data Low Byte
00009076 7B 97              		R3 = R3 lsr 4;
00009077 7B 97              		R3 = R3 lsr 4;
00009078 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data High Byte
                            		
                            	
                            	?L_Check_TXIF:
0000907A 11 93 65 20        		R1 = [P_SPI_Misc];	
0000907C 50 C2              		test R1, C_SPI_Busy_Flag;
0000907D 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000907E 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            	
00009080 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009082 12 95 64 20        		R2 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009084 13 97 64 20        		R3 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009086 14 99 64 20        		R4 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00009088 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000908A 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            		
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000908C 40 F0 03 91        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000908E 41 C2              		test R1, C_Flash_Busy;
0000908F 44 4E              		jnz ?L_Check_Busy;
                            		
00009090 A8 90              		pop R1, R5 from [SP];
00009091 90 9A              		retf;	
                            		.endp
                            	
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendNWords
                            	// Description : Send N words to external memory from internal buffer
                            	// Destory     : None
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	 _SPI_SendNWords:	.proc
00009092 08 09 04 00        		R4 = SP + 4;
00009094 D4 92              		R1 = [R4++];
00009095 D4 94              		R2 = [R4++];
00009096 D4 96              		R3 = [R4++];
00009097 C4 98              		R4 = [R4];
                            	F_SPI_SendNWords:
00009098 A8 DA              		push R1, R5 to [SP];
                            	
                            	?L_WriteData:
                            		
00009099 40 F0 E0 90        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
0000909B 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
                            	
0000909D 42 9A              		R5 = C_SPI_Flash_Page_Program;
0000909E 1D DB 62 20        		[P_SPI_TX_Data] = R5;
000090A0 1C D9 62 20        		[P_SPI_TX_Data] = R4;
000090A2 7B 9B              		R5 = R3 lsr 4;
000090A3 7D 9B              		R5 = R5 lsr 4;					// Address Middle
000090A4 1D DB 62 20        		[P_SPI_TX_Data] = R5;
000090A6 0B BB FF 00        		R5 = R3 & 0xFF;					// Address Low
000090A8 1D DB 62 20        		[P_SPI_TX_Data] = R5;
                            		
                            	
                            	?L_Check_TXIF:
000090AA 15 9B 65 20        		R5 = [P_SPI_Misc];	
000090AC 50 CA              		test R5, C_SPI_Busy_Flag;
000090AD 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
000090AE 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
000090B0 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
000090B2 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
000090B4 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            	
                            	?L_SendDataLoop:
000090B6 D1 9A              		R5 = [R1++];
000090B7 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data Low Byte
000090B9 7D 9B              		R5 = R5 lsr 4;
000090BA 7D 9B              		R5 = R5 lsr 4;
000090BB 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data High Byte
                            	
                            	?L_Check_TXIF_2:
000090BD 15 9B 65 20        		R5 = [P_SPI_Misc];	
000090BF 50 CA              		test R5, C_SPI_Busy_Flag;
000090C0 44 4E              		jnz ?L_Check_TXIF_2;
                            	
000090C1 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
000090C3 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO
000090C5 41 24              		R2 -= 1;
000090C6 11 5E              		jz ?L_SendDataEnd;
000090C7 42 06              		R3 += 2;
000090C8 0B C7 FF 00        		test R3, 0x00FF;
000090CA 55 4E              		jnz ?L_SendDataLoop;
                            	?L_JumpToNextPage:
000090CB 40 46              		cmp R3, 0x0000;
000090CC 01 4E              		jne ?L_WriteCurrentPage;
000090CD 41 08              		R4 += 1;
                            	?L_WriteCurrentPage:
000090CE 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
000090D0 88 D2              		push R1 to [SP];
                            	?L_Check_Busy_1:					// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
000090D1 40 F0 03 91        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
000090D3 41 C2              		test R1, C_Flash_Busy;
000090D4 44 4E              		jnz ?L_Check_Busy_1;
000090D5 88 90              		pop R1 from [SP];
000090D6 0F 9F 99 90        		pc = ?L_WriteData;
                            	
                            	?L_SendDataEnd:
000090D8 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
                            	
                            	
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
000090DA 40 F0 03 91        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
000090DC 41 C2              		test R1, C_Flash_Busy;
000090DD 44 4E              		jnz ?L_Check_Busy;
                            		
                            	
000090DE A8 90              		pop R1, R5 from [SP];
000090DF 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
