                            	//****************************************************************
                            	// Function    : F_SPI_Flash_Chip_Erase
                            	// Description : Erase hole chip of flash
                            	// Destory     : None
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Flash_Chip_Erase:	.proc
                            	F_SPI_Flash_Chip_Erase:
0000916D 88 D2              		push R1 to [SP];
0000916E 40 F0 E0 90        		call F_Flash_Write_Enable;		// Enable chip erase command 
                            		
00009170 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
00009172 09 93 60 00        		R1 = C_SPI_Flash_Chip_Erase;
00009174 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            	
                            	
                            	?L_Check_TXIF:
00009176 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
00009178 50 C2              		test R1, C_SPI_Busy_Flag;
00009179 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000917A 11 93 64 20        		R1 = [P_SPI_RX_Data];
                            	
                            	?L_Check_Busy:						// Wait untill chip has been erased successfully (about 128 to 256 seconds)
0000917C 09 93 55 55        		R1 = C_Watchdog_Clear;
0000917E 19 D3 34 20        	    [P_Watchdog_Clear] = R1;
00009180 40 F0 03 91        		call F_SPI_Read_Status_Register;
00009182 41 C2              		test R1, C_Flash_Busy;
00009183 48 4E              		jnz ?L_Check_Busy;
                            	
00009184 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
00009186 88 90              		pop R1 from [SP];
00009187 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_Read_Flash_ID
                            	// Description : Read flash manufacturer,memory and individual device ID
                            	// Destory     : None
                            	// Parameter   : None
                            	// Return      : R1 = Manufacturer and memory ID, R2 = Individual Device ID
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Read_Flash_ID:	.proc
                            	F_SPI_Read_Flash_ID:
00009188 90 D8              		push R3, R4 to [SP];
00009189 6C F2 00 20        		clrb [P_IOA_Data], C_SPI_CS_IO;		// enable SPI Flash
0000918B 0C 99 9F 00        		R4 = C_SPI_Flash_Read_ID;
0000918D 1C D9 62 20        		[P_SPI_TX_Data] = R4;
                            			
0000918F 40 92              		R1 = 0x00;					// Read One Byte Data dummy clock
00009190 19 D3 62 20        		[P_SPI_TX_Data] = R1;
00009192 19 D3 62 20        		[P_SPI_TX_Data] = R1;
00009194 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:
00009196 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
00009198 50 C2              		test R1, C_SPI_Busy_Flag;
00009199 44 4E              		jnz ?L_Check_TXIF;
                            	
                            	
0000919A 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear Dummy Data in RX FIFO at Read Cmd transmitted   
0000919C 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Read Manufacturer ID
0000919E 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Read Memory ID
000091A0 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Read Individual Device ID
000091A2 5B 93              		R1 = R3 LSL 4;
000091A3 59 93              		R1 = R1 LSL 4;
000091A4 02 A3              		R1 |= R2;
000091A5 04 95              		R2 = R4;
                            	
000091A6 5C F2 00 20        		setb [P_IOA_Data], C_SPI_CS_IO;	// disable SPI Flash
000091A8 90 94              		pop R3, R4 from [SP];
000091A9 90 9A              		retf;
                            		.endp
                            	
