00009497                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SACM_A1600_Init_
                            	// Description : Hardware initilazation for A1600, called by library
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	F_SACM_A1600_Init_:	.proc
00009497 45 F1              		FIR_MOV OFF;
                            		
00009498 4F 92              		R1 = C_TimerA_FPLL;				// TimerA CKA=Fosc/2 CKB=1 Tout:off
00009499 19 D3 16 20        		[P_Timer_Ctrl] = R1;
0000949B 09 93 00 E8        		R1= C_A1600_Timer_Setting_X1;	// TimerA setting
0000949D 19 D3 10 20        		[P_TimerA_Data] = R1;
0000949F 19 D3 11 20        		[P_TimerA_CNTR] = R1;
                            	
                            	
000094A1 45 92              		R1 = C_DAC_Enable | C_DAC_TMR_Sel_TimerA;
000094A2 19 D3 40 20        		[P_DAC_Ctrl] = R1;		// b2 of P_DAC_Ctrl must be set to 1 in GPCE500A. GPCE060A doesn't use this bit
                            	
                            	
000094A4 11 93 50 20        		R1 = [P_INT_Ctrl];
000094A6 09 A3 00 10        		R1 |= C_IRQ0_TMA;
000094A8 19 D3 50 20        		[P_INT_Ctrl] = R1;
                            	
000094AA 11 93 52 20        		R1 = [P_FIQ_Sel];
000094AC 09 A3 00 10        		R1 |= C_IRQ0_TMA;
000094AE 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            	
000094B0 4E F1              		FIQ on;
000094B1 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_USER_A1600_Volume
                            	// Description : Set speech volume
                            	// Destory     : R1
                            	// Parameter   : R1: volume index
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	 _USER_A1600_Volume: .proc
000094B2 08 03 03 00        		R1 = SP + 3;
000094B4 C1 92              		R1 = [R1];								// volume index
                            	F_USER_A1600_Volume:
000094B5 09 03 6D 80        		R1 += T_SACM_A1600_Volume_Level;		// loop up volume table
000094B7 C1 92              		R1 = [R1];
000094B8 40 F0 57 91        		call F_SACM_A1600_Volume;
000094BA 90 9A              		retf
                            		.endp
                            	
