                            	// Destory     : None
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_Flash_Write_Enable:	.proc
                            	F_Flash_Write_Enable:
00008D9F 88 D2              		push r1 to [sp];
                            	
00008DA0 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
00008DA2 46 92              		R1 = C_SPI_Flash_Write_Enable;
00008DA3 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            	
                            	
                            	?L_Check_TXIF:
00008DA5 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
00008DA7 50 C2              		test R1, C_SPI_Busy_Flag;
00008DA8 44 4E              		jnz ?L_Check_TXIF;
                            	
                            	
00008DA9 65 F1              		nop;
00008DAA 65 F1              		nop;
00008DAB 65 F1              		nop;
00008DAC 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
                            		
00008DAE 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
00008DB0 88 90              		pop r1 from [sp];
00008DB1 90 9A              		retf;
                            		.endp
                            	
