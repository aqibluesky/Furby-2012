00008C65                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SPI_Initial
                            	// Description : Initial SPI interface
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Initial: .proc
                            	F_SPI_Initial:
00008C65 90 D4              		push R1, R2 to [SP];
                            	
00008C66 11 93 02 20        		R1 = [P_IOA_Dir];
00008C68 09 B3 FF 7F        		R1 &= ~C_SPI_SI_Bit;
00008C6A 09 A3 00 70        		R1 |= C_SPI_DO_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00008C6C 19 D3 02 20        		[P_IOA_Dir] = R1;
                            		
00008C6E 11 93 03 20        		R1 = [P_IOA_Attrib];
00008C70 09 A3 00 F0        		R1 |= C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00008C72 19 D3 03 20        		[P_IOA_Attrib] = R1;
                            		
00008C74 11 93 01 20        		R1 = [P_IOA_Buffer];
00008C76 09 B3 FF 1F        		R1 &= ~(C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit );
00008C78 09 A3 00 10        		R1 |= C_SPI_CS_Bit;
00008C7A 19 D3 01 20        		[P_IOA_Buffer] = R1;
                            	
                            	
00008C7C 11 93 06 20        		R1 = [P_IOB_Dir];			// IOB as Input Floating 
00008C7E 40 B2              		R1 &= 0x0000;
00008C7F 19 D3 06 20        		[P_IOB_Dir] = R1;
00008C81 11 93 07 20        		R1 = [P_IOB_Attrib];
00008C83 09 A3 FF FF        		R1 |= 0xFFFF;
00008C85 19 D3 07 20        		[P_IOB_Attrib] = R1;
00008C87 11 93 05 20        		R1 = [P_IOB_Buffer];
00008C89 40 B2              		R1 &= 0x0000;
00008C8A 19 D3 05 20        		[P_IOB_Buffer] = R1;
                            		
00008C8C 11 93 08 20        		R1 = [P_IO_Ctrl];
00008C8E 09 A3 00 04        		R1 |= C_SPI_CS_Hardware;
00008C90 19 D3 08 20        		[P_IO_Ctrl] = R1;
                            		
00008C92 09 93 00 08        		R1 = C_SPI_Reset;		// software reset SPI
00008C94 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            	
00008C96 09 93 02 80        		R1 = C_SPI_Enable | C_MasterMode | C_SCKSEL_FPLL_Div_8;
00008C98 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            		
00008C9A 09 93 00 80        		R1 = C_SPI_TX_INT_Flag | C_SPI_TX_INT_DIS | C_SPI_TX_FIFO_Level_0;
00008C9C 19 D3 61 20        		[P_SPI_TX_Status] = R1;
                            		
00008C9E 09 93 00 80        		R1 = C_SPI_RX_INT_Flag | C_SPI_RX_INT_DIS | C_SPI_RX_FIFO_Level_1;
00008CA0 19 D3 63 20        		[P_SPI_RX_Status] = R1;
                            		
00008CA2 11 93 50 20        		R1 = [P_INT_Ctrl];
00008CA4 19 D3 50 20        		[P_INT_Ctrl] = R1;
00008CA6 40 92              		R1 = 0x0000;
00008CA7 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            		
00008CA9 90 90              		pop R1, R2 from [SP];
00008CAA 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadAWord
                            	// Description : Read a word data from flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High	
                            	// Return      : R1 = one word of data read
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_ReadAWord:	.proc
00008CAB 08 05 03 00        		R2 = SP + 3;
00008CAD D2 92              		R1 = [R2++];
00008CAE C2 94              		R2 = [R2]; 
                            		
                            	F_SPI_ReadAWord:
00008CAF 98 D8              		push r2, r4 to [sp];
00008CB0 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
00008CB2 43 98              		R4 = C_SPI_Flash_Read_CMD;
00008CB3 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008CB5 0A B5 FF 00        		R2 &= 0xFF;					// Address High
00008CB7 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00008CB9 79 99              		R4 = R1 lsr 4;
00008CBA 7C 99              		R4 = R4 lsr 4;				// Address Middle
00008CBB 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008CBD 09 B3 FF 00        		R1 &= 0xFF;					// Address Low
00008CBF 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
00008CC1 09 93 55 00        		R1 = 0x55;					// Read One Word Data dummy clock
00008CC3 19 D3 62 20        		[P_SPI_TX_Data] = R1;
00008CC5 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00008CC7 11 93 65 20        		R1 = [P_SPI_Misc];	
00008CC9 50 C2              		test R1, C_SPI_Busy_Flag;
00008CCA 44 4E              		jnz ?L_Check_TXIF;
00008CCB 19 D3 65 20        		[P_SPI_Misc] = R1;
                            		
                            	
00008CCD 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
00008CCF 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00008CD1 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00008CD3 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
00008CD5 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Received Valid Data High from slave SPI device
00008CD7 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Received Valid Data Low from slave SPI device
00008CD9 59 93              		R1 = R1 LSL 4;
00008CDA 59 93              		R1 = R1 LSL 4;
00008CDB 02 A3              		R1 |= R2;					// Return one word data read from FLASH
                            	
00008CDC 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
00008CDE 98 92              		pop R2, R4 from [SP];
00008CDF 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadNWords
                            	// Description : Get N words from external memory to buffer
                            	// Destory     : R1
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	_SPI_ReadNWords:      .proc
00008CE0 08 09 03 00        	    R4 = SP + 3;
00008CE2 D4 92              	    R1 = [R4++];							// buffer address
00008CE3 D4 94              	    R2 = [R4++];							// data length
00008CE4 D4 96              	    R3 = [R4++];							// external memory address low byte
00008CE5 C4 98              	    R4 = [R4];							// external memory address high byte
                            	
                            	F_SPI_ReadNWords:
00008CE6 A8 DA              		push R1, R5 to [SP];
00008CE7 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
00008CE9 43 9A              		R5 = C_SPI_Flash_Read_CMD;
00008CEA 1D DB 62 20        		[P_SPI_TX_Data] = R5;
00008CEC 0C B9 FF 00        		R4 &= 0xFF;					// Address High
00008CEE 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008CF0 7B 99              		R4 = R3 lsr 4;
00008CF1 7C 99              		R4 = R4 lsr 4;				// Address Middle
00008CF2 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008CF4 0B B7 FF 00        		R3 &= 0xFF;					// Address Low
00008CF6 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00008CF8 15 9B 65 20        		R5 = [P_SPI_Misc];	
00008CFA 50 CA              		test R5, C_SPI_Busy_Flag;
00008CFB 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00008CFC 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
00008CFE 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00008D00 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00008D02 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
                            	
                            	?L_GetNWordLoop:
00008D04 1B D7 62 20        		[P_SPI_TX_Data] = R3;		// Read One Word Data dummy clock
00008D06 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            	
                            	?L_GetNWordLoop_Check_TXIF:		// Wait untill command and address have been sent out
00008D08 15 9B 65 20        		R5 = [P_SPI_Misc];	
00008D0A 50 CA              		test R5, C_SPI_Busy_Flag;
00008D0B 44 4E              		jnz ?L_GetNWordLoop_Check_TXIF;
                            	
                            		
00008D0C 13 97 64 20        		R3 = [P_SPI_RX_Data];		// low byte data
00008D0E 14 99 64 20        		R4 = [P_SPI_RX_Data];		// high byte data
00008D10 5C 99              		R4 = R4 lsl 4;
00008D11 5C 99              		R4 = R4 lsl 4;
00008D12 03 A9              		R4 |= R3;					// Return one word data read from FLASH
00008D13 D1 D8              		[R1++] = R4;
00008D14 41 24              		R2 -= 1;
00008D15 52 4E              		jnz ?L_GetNWordLoop;
                            	
00008D16 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            		
                            	?_Finish_Last:
00008D18 A8 90              		pop R1, R5 from [SP];
                            	
00008D19 90 9A              		retf;
                            	.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendAWord
                            	// Description : Write a word data to flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High, R3 = one word of data
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_SendAWord:	.proc
00008D1A 08 07 03 00        		R3 = SP + 3;
00008D1C D3 92              		R1 = [R3++];
00008D1D D3 94              		R2 = [R3++];
00008D1E C3 96              		R3 = [R3];
                            		
                            	F_SPI_SendAWord:
00008D1F A8 DA              		push R1, R5 to [SP];
                            		
00008D20 40 F0 9F 8D        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
00008D22 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
00008D24 42 98              		R4 = C_SPI_Flash_Page_Program;
00008D25 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008D27 0A B5 FF 00        		R2 &= 0xFF;						// Address High
00008D29 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00008D2B 79 99              		R4 = R1 lsr 4;
00008D2C 7C 99              		R4 = R4 lsr 4;					// Address Middle
00008D2D 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008D2F 09 B3 FF 00        		R1 &= 0xFF;						// Address Low
00008D31 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
00008D33 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data Low Byte
00008D35 7B 97              		R3 = R3 lsr 4;
00008D36 7B 97              		R3 = R3 lsr 4;
00008D37 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data High Byte
                            		
                            	
                            	?L_Check_TXIF:
00008D39 11 93 65 20        		R1 = [P_SPI_Misc];	
00008D3B 50 C2              		test R1, C_SPI_Busy_Flag;
00008D3C 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00008D3D 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
00008D3F 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D41 12 95 64 20        		R2 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D43 13 97 64 20        		R3 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D45 14 99 64 20        		R4 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D47 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D49 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            		
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
00008D4B 40 F0 C2 8D        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
00008D4D 41 C2              		test R1, C_Flash_Busy;
00008D4E 44 4E              		jnz ?L_Check_Busy;
                            		
00008D4F A8 90              		pop R1, R5 from [SP];
00008D50 90 9A              		retf;	
                            		.endp
                            	
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendNWords
                            	// Description : Send N words to external memory from internal buffer
                            	// Destory     : None
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	 _SPI_SendNWords:	.proc
00008D51 08 09 04 00        		R4 = SP + 4;
00008D53 D4 92              		R1 = [R4++];
00008D54 D4 94              		R2 = [R4++];
00008D55 D4 96              		R3 = [R4++];
00008D56 C4 98              		R4 = [R4];
                            	F_SPI_SendNWords:
00008D57 A8 DA              		push R1, R5 to [SP];
                            	
                            	?L_WriteData:
                            		
00008D58 40 F0 9F 8D        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
00008D5A 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
00008D5C 42 9A              		R5 = C_SPI_Flash_Page_Program;
00008D5D 1D DB 62 20        		[P_SPI_TX_Data] = R5;
00008D5F 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00008D61 7B 9B              		R5 = R3 lsr 4;
00008D62 7D 9B              		R5 = R5 lsr 4;					// Address Middle
00008D63 1D DB 62 20        		[P_SPI_TX_Data] = R5;
00008D65 0B BB FF 00        		R5 = R3 & 0xFF;					// Address Low
00008D67 1D DB 62 20        		[P_SPI_TX_Data] = R5;
                            		
                            	
                            	?L_Check_TXIF:
00008D69 15 9B 65 20        		R5 = [P_SPI_Misc];	
00008D6B 50 CA              		test R5, C_SPI_Busy_Flag;
00008D6C 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00008D6D 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D6F 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D71 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D73 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            	
                            	?L_SendDataLoop:
00008D75 D1 9A              		R5 = [R1++];
00008D76 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data Low Byte
00008D78 7D 9B              		R5 = R5 lsr 4;
00008D79 7D 9B              		R5 = R5 lsr 4;
00008D7A 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data High Byte
                            	
                            	?L_Check_TXIF_2:
00008D7C 15 9B 65 20        		R5 = [P_SPI_Misc];	
00008D7E 50 CA              		test R5, C_SPI_Busy_Flag;
00008D7F 44 4E              		jnz ?L_Check_TXIF_2;
                            	
00008D80 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
00008D82 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO
00008D84 41 24              		R2 -= 1;
00008D85 11 5E              		jz ?L_SendDataEnd;
00008D86 42 06              		R3 += 2;
00008D87 0B C7 FF 00        		test R3, 0x00FF;
00008D89 55 4E              		jnz ?L_SendDataLoop;
                            	?L_JumpToNextPage:
00008D8A 40 46              		cmp R3, 0x0000;
00008D8B 01 4E              		jne ?L_WriteCurrentPage;
00008D8C 41 08              		R4 += 1;
                            	?L_WriteCurrentPage:
00008D8D 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
00008D8F 88 D2              		push R1 to [SP];
                            	?L_Check_Busy_1:					// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
00008D90 40 F0 C2 8D        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
00008D92 41 C2              		test R1, C_Flash_Busy;
00008D93 44 4E              		jnz ?L_Check_Busy_1;
00008D94 88 90              		pop R1 from [SP];
00008D95 0F 9F 58 8D        		pc = ?L_WriteData;
                            	
                            	?L_SendDataEnd:
00008D97 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
                            	
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
00008D99 40 F0 C2 8D        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
00008D9B 41 C2              		test R1, C_Flash_Busy;
00008D9C 44 4E              		jnz ?L_Check_Busy;
                            		
                            	
00008D9D A8 90              		pop R1, R5 from [SP];
00008D9E 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_Flash_Write_Enable
