0000BFDC                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SPI_Initial
                            	// Description : Initial SPI interface
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Initial: .proc
                            	F_SPI_Initial:
0000BFDC 90 D4              		push R1, R2 to [SP];
                            	
0000BFDD 11 93 02 20        		R1 = [P_IOA_Dir];
0000BFDF 09 B3 FF 7F        		R1 &= ~C_SPI_SI_Bit;
0000BFE1 09 A3 00 70        		R1 |= C_SPI_DO_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
0000BFE3 19 D3 02 20        		[P_IOA_Dir] = R1;
                            		
0000BFE5 11 93 03 20        		R1 = [P_IOA_Attrib];
0000BFE7 09 A3 00 F0        		R1 |= C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
0000BFE9 19 D3 03 20        		[P_IOA_Attrib] = R1;
                            		
0000BFEB 11 93 01 20        		R1 = [P_IOA_Buffer];
0000BFED 09 B3 FF 1F        		R1 &= ~(C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit );
0000BFEF 09 A3 00 10        		R1 |= C_SPI_CS_Bit;
0000BFF1 19 D3 01 20        		[P_IOA_Buffer] = R1;
                            	
                            	
0000BFF3 11 93 06 20        		R1 = [P_IOB_Dir];			// IOB as Input Floating 
0000BFF5 40 B2              		R1 &= 0x0000;
0000BFF6 19 D3 06 20        		[P_IOB_Dir] = R1;
0000BFF8 11 93 07 20        		R1 = [P_IOB_Attrib];
0000BFFA 09 A3 FF FF        		R1 |= 0xFFFF;
0000BFFC 19 D3 07 20        		[P_IOB_Attrib] = R1;
0000BFFE 11 93 05 20        		R1 = [P_IOB_Buffer];
0000C000 40 B2              		R1 &= 0x0000;
0000C001 19 D3 05 20        		[P_IOB_Buffer] = R1;
                            		
0000C003 11 93 08 20        		R1 = [P_IO_Ctrl];
0000C005 09 A3 00 04        		R1 |= C_SPI_CS_Hardware;
0000C007 19 D3 08 20        		[P_IO_Ctrl] = R1;
                            		
0000C009 09 93 00 08        		R1 = C_SPI_Reset;		// software reset SPI
0000C00B 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            	
0000C00D 09 93 02 80        		R1 = C_SPI_Enable | C_MasterMode | C_SCKSEL_FPLL_Div_8;
0000C00F 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            		
0000C011 09 93 00 80        		R1 = C_SPI_TX_INT_Flag | C_SPI_TX_INT_DIS | C_SPI_TX_FIFO_Level_0;
0000C013 19 D3 61 20        		[P_SPI_TX_Status] = R1;
                            		
0000C015 09 93 00 80        		R1 = C_SPI_RX_INT_Flag | C_SPI_RX_INT_DIS | C_SPI_RX_FIFO_Level_1;
0000C017 19 D3 63 20        		[P_SPI_RX_Status] = R1;
                            		
0000C019 11 93 50 20        		R1 = [P_INT_Ctrl];
0000C01B 19 D3 50 20        		[P_INT_Ctrl] = R1;
0000C01D 40 92              		R1 = 0x0000;
0000C01E 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            		
0000C020 90 90              		pop R1, R2 from [SP];
0000C021 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadAWord
                            	// Description : Read a word data from flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High	
                            	// Return      : R1 = one word of data read
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_ReadAWord:	.proc
0000C022 08 05 03 00        		R2 = SP + 3;
0000C024 D2 92              		R1 = [R2++];
0000C025 C2 94              		R2 = [R2]; 
                            		
                            	F_SPI_ReadAWord:
0000C026 98 D8              		push r2, r4 to [sp];
0000C027 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
0000C029 43 98              		R4 = C_SPI_Flash_Read_CMD;
0000C02A 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C02C 0A B5 FF 00        		R2 &= 0xFF;					// Address High
0000C02E 1A D5 62 20        		[P_SPI_TX_Data] = R2;
0000C030 79 99              		R4 = R1 lsr 4;
0000C031 7C 99              		R4 = R4 lsr 4;				// Address Middle
0000C032 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C034 09 B3 FF 00        		R1 &= 0xFF;					// Address Low
0000C036 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
0000C038 09 93 55 00        		R1 = 0x55;					// Read One Word Data dummy clock
0000C03A 19 D3 62 20        		[P_SPI_TX_Data] = R1;
0000C03C 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
0000C03E 11 93 65 20        		R1 = [P_SPI_Misc];	
0000C040 50 C2              		test R1, C_SPI_Busy_Flag;
0000C041 44 4E              		jnz ?L_Check_TXIF;
0000C042 19 D3 65 20        		[P_SPI_Misc] = R1;
                            		
                            	
0000C044 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
0000C046 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
0000C048 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
0000C04A 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
0000C04C 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Received Valid Data High from slave SPI device
0000C04E 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Received Valid Data Low from slave SPI device
0000C050 59 93              		R1 = R1 LSL 4;
0000C051 59 93              		R1 = R1 LSL 4;
0000C052 02 A3              		R1 |= R2;					// Return one word data read from FLASH
                            	
0000C053 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
0000C055 98 92              		pop R2, R4 from [SP];
0000C056 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadNWords
                            	// Description : Get N words from external memory to buffer
                            	// Destory     : R1
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	_SPI_ReadNWords:      .proc
0000C057 08 09 03 00        	    R4 = SP + 3;
0000C059 D4 92              	    R1 = [R4++];							// buffer address
0000C05A D4 94              	    R2 = [R4++];							// data length
0000C05B D4 96              	    R3 = [R4++];							// external memory address low byte
0000C05C C4 98              	    R4 = [R4];							// external memory address high byte
                            	
                            	F_SPI_ReadNWords:
0000C05D A8 DA              		push R1, R5 to [SP];
0000C05E 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
0000C060 43 9A              		R5 = C_SPI_Flash_Read_CMD;
0000C061 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000C063 0C B9 FF 00        		R4 &= 0xFF;					// Address High
0000C065 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C067 7B 99              		R4 = R3 lsr 4;
0000C068 7C 99              		R4 = R4 lsr 4;				// Address Middle
0000C069 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C06B 0B B7 FF 00        		R3 &= 0xFF;					// Address Low
0000C06D 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
0000C06F 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000C071 50 CA              		test R5, C_SPI_Busy_Flag;
0000C072 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000C073 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
0000C075 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
0000C077 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
0000C079 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
                            	
                            	?L_GetNWordLoop:
0000C07B 1B D7 62 20        		[P_SPI_TX_Data] = R3;		// Read One Word Data dummy clock
0000C07D 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            	
                            	?L_GetNWordLoop_Check_TXIF:		// Wait untill command and address have been sent out
0000C07F 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000C081 50 CA              		test R5, C_SPI_Busy_Flag;
0000C082 44 4E              		jnz ?L_GetNWordLoop_Check_TXIF;
                            	
                            		
0000C083 13 97 64 20        		R3 = [P_SPI_RX_Data];		// low byte data
0000C085 14 99 64 20        		R4 = [P_SPI_RX_Data];		// high byte data
0000C087 5C 99              		R4 = R4 lsl 4;
0000C088 5C 99              		R4 = R4 lsl 4;
0000C089 03 A9              		R4 |= R3;					// Return one word data read from FLASH
0000C08A D1 D8              		[R1++] = R4;
0000C08B 41 24              		R2 -= 1;
0000C08C 52 4E              		jnz ?L_GetNWordLoop;
                            	
0000C08D 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            		
                            	?_Finish_Last:
0000C08F A8 90              		pop R1, R5 from [SP];
                            	
0000C090 90 9A              		retf;
                            	.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendAWord
                            	// Description : Write a word data to flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High, R3 = one word of data
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_SendAWord:	.proc
0000C091 08 07 03 00        		R3 = SP + 3;
0000C093 D3 92              		R1 = [R3++];
0000C094 D3 94              		R2 = [R3++];
0000C095 C3 96              		R3 = [R3];
                            		
                            	F_SPI_SendAWord:
0000C096 A8 DA              		push R1, R5 to [SP];
                            		
0000C097 40 F0 16 C1        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
0000C099 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
0000C09B 42 98              		R4 = C_SPI_Flash_Page_Program;
0000C09C 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C09E 0A B5 FF 00        		R2 &= 0xFF;						// Address High
0000C0A0 1A D5 62 20        		[P_SPI_TX_Data] = R2;
0000C0A2 79 99              		R4 = R1 lsr 4;
0000C0A3 7C 99              		R4 = R4 lsr 4;					// Address Middle
0000C0A4 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C0A6 09 B3 FF 00        		R1 &= 0xFF;						// Address Low
0000C0A8 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
0000C0AA 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data Low Byte
0000C0AC 7B 97              		R3 = R3 lsr 4;
0000C0AD 7B 97              		R3 = R3 lsr 4;
0000C0AE 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data High Byte
                            		
                            	
                            	?L_Check_TXIF:
0000C0B0 11 93 65 20        		R1 = [P_SPI_Misc];	
0000C0B2 50 C2              		test R1, C_SPI_Busy_Flag;
0000C0B3 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000C0B4 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
0000C0B6 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0B8 12 95 64 20        		R2 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0BA 13 97 64 20        		R3 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0BC 14 99 64 20        		R4 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0BE 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0C0 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            		
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000C0C2 40 F0 39 C1        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000C0C4 41 C2              		test R1, C_Flash_Busy;
0000C0C5 44 4E              		jnz ?L_Check_Busy;
                            		
0000C0C6 A8 90              		pop R1, R5 from [SP];
0000C0C7 90 9A              		retf;	
                            		.endp
                            	
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendNWords
                            	// Description : Send N words to external memory from internal buffer
                            	// Destory     : None
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	 _SPI_SendNWords:	.proc
0000C0C8 08 09 04 00        		R4 = SP + 4;
0000C0CA D4 92              		R1 = [R4++];
0000C0CB D4 94              		R2 = [R4++];
0000C0CC D4 96              		R3 = [R4++];
0000C0CD C4 98              		R4 = [R4];
                            	F_SPI_SendNWords:
0000C0CE A8 DA              		push R1, R5 to [SP];
                            	
                            	?L_WriteData:
                            		
0000C0CF 40 F0 16 C1        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
0000C0D1 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
0000C0D3 42 9A              		R5 = C_SPI_Flash_Page_Program;
0000C0D4 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000C0D6 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000C0D8 7B 9B              		R5 = R3 lsr 4;
0000C0D9 7D 9B              		R5 = R5 lsr 4;					// Address Middle
0000C0DA 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000C0DC 0B BB FF 00        		R5 = R3 & 0xFF;					// Address Low
0000C0DE 1D DB 62 20        		[P_SPI_TX_Data] = R5;
                            		
                            	
                            	?L_Check_TXIF:
0000C0E0 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000C0E2 50 CA              		test R5, C_SPI_Busy_Flag;
0000C0E3 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000C0E4 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0E6 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0E8 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0EA 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            	
                            	?L_SendDataLoop:
0000C0EC D1 9A              		R5 = [R1++];
0000C0ED 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data Low Byte
0000C0EF 7D 9B              		R5 = R5 lsr 4;
0000C0F0 7D 9B              		R5 = R5 lsr 4;
0000C0F1 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data High Byte
                            	
                            	?L_Check_TXIF_2:
0000C0F3 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000C0F5 50 CA              		test R5, C_SPI_Busy_Flag;
0000C0F6 44 4E              		jnz ?L_Check_TXIF_2;
                            	
0000C0F7 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000C0F9 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO
0000C0FB 41 24              		R2 -= 1;
0000C0FC 11 5E              		jz ?L_SendDataEnd;
0000C0FD 42 06              		R3 += 2;
0000C0FE 0B C7 FF 00        		test R3, 0x00FF;
0000C100 55 4E              		jnz ?L_SendDataLoop;
                            	?L_JumpToNextPage:
0000C101 40 46              		cmp R3, 0x0000;
0000C102 01 4E              		jne ?L_WriteCurrentPage;
0000C103 41 08              		R4 += 1;
                            	?L_WriteCurrentPage:
0000C104 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
0000C106 88 D2              		push R1 to [SP];
                            	?L_Check_Busy_1:					// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000C107 40 F0 39 C1        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000C109 41 C2              		test R1, C_Flash_Busy;
0000C10A 44 4E              		jnz ?L_Check_Busy_1;
0000C10B 88 90              		pop R1 from [SP];
0000C10C 0F 9F CF C0        		pc = ?L_WriteData;
                            	
                            	?L_SendDataEnd:
0000C10E 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
                            	
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000C110 40 F0 39 C1        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000C112 41 C2              		test R1, C_Flash_Busy;
0000C113 44 4E              		jnz ?L_Check_Busy;
                            		
                            	
0000C114 A8 90              		pop R1, R5 from [SP];
0000C115 90 9A              		retf;
                            		.endp
                            	
