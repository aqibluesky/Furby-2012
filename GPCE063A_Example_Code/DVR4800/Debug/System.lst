                            	//****************************************************************
                            	// Function    : F_System_ServiceLoop
                            	// Description : Key scan and watchdog clear
                            	// Destory     : R1, R2
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	 _System_ServiceLoop: .proc
                            	F_System_ServiceLoop:
0000A50C 40 F0 13 A5        		call F_Key_DebounceCnt_Down;		// debounce counter countdown
0000A50E 40 F0 1A A5        		call F_Key_Scan_ServiceLoop;		// key scan
0000A510 40 F0 07 A5        		call F_WatchdogClear;				// clear watchdog register
0000A512 90 9A              		retf;
