                            	//****************************************************************
                            	// Function    : F_SP_SwitchChannel
                            	// Description : Switch A/D channel
                            	// Destory     : R1, R2
                            	// Parameter   : R1 = A/D channel
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SP_SwitchChannel: .proc
0000A4CE 08 03 03 00        		R1 = SP + 3;
0000A4D0 C1 92              		R1 = [R1];
                            	F_SP_SwitchChannel:
0000A4D1 40 42              		cmp R1, 0;
0000A4D2 0A 4E              		jnz ?L_LineIn;
0000A4D3 09 93 00 C0        		R1 = C_ADC_Auto_Scan_Busy | C_ADC_Busy;
                            	?L_Wait_AD_Ready_MIC:
0000A4D5 11 C3 70 20        		test R1, [P_ADC_Ctrl];
0000A4D7 43 4E              		jnz ?L_Wait_AD_Ready_MIC;
                            	
0000A4D8 09 93 EC 10        		R1 = C_ADC_Enable | C_AGC_Enable | C_ADC_CLK_FPLL_Div_32 | C_ADC_Bias_Enable | C_ADC_MIC_Enable | C_ADC_Timer_A
0000A4DA 19 D3 70 20        		[P_ADC_Ctrl] = R1;
                            	
0000A4DC 0C EE              		jmp ?L_SwitchChannelEnd;
                            		
                            	?L_LineIn:
0000A4DD 09 93 00 C0        		R1 = C_ADC_Auto_Scan_Busy | C_ADC_Busy;
                            	?L_Wait_AD_Ready_LinIn:
0000A4DF 11 C3 70 20        		test R1, [P_ADC_Ctrl];
0000A4E1 43 4E              		jnz ?L_Wait_AD_Ready_LinIn;
                            		
0000A4E2 41 92              		R1 = C_ADC_AN0_Enable
0000A4E3 19 D3 72 20        		[P_ADC_LineIn_BitCtrl] = R1
                            		
                            		
0000A4E5 09 93 D4 10        		R1 =C_ADC_Bias_Enable | C_ADC_CLK_FPLL_Div_32 | C_ADC_Timer_A | C_ADC_Manual_AN0 | C_ADC_Enable | C_ADC_Ch0_in_LineinPB0;
0000A4E7 19 D3 70 20        		[P_ADC_Ctrl] = R1;
                            	
                            	?L_SwitchChannelEnd:
0000A4E9 90 9A              		retf;
                            		.endp
                            	
                            	
