00009F39                    	.CODE
                            	//****************************************************************
                            	// Function    : F_SPI_Initial
                            	// Description : Initial SPI interface
                            	// Destory     : R1
                            	// Parameter   : None
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Initial: .proc
                            	F_SPI_Initial:
00009F39 90 D4              		push R1, R2 to [SP];
                            	
00009F3A 11 93 02 20        		R1 = [P_IOA_Dir];
00009F3C 09 B3 FF 7F        		R1 &= ~C_SPI_SI_Bit;
00009F3E 09 A3 00 70        		R1 |= C_SPI_DO_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00009F40 19 D3 02 20        		[P_IOA_Dir] = R1;
                            		
00009F42 11 93 03 20        		R1 = [P_IOA_Attrib];
00009F44 09 A3 00 F0        		R1 |= C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit | C_SPI_CS_Bit;
00009F46 19 D3 03 20        		[P_IOA_Attrib] = R1;
                            		
00009F48 11 93 01 20        		R1 = [P_IOA_Buffer];
00009F4A 09 B3 FF 1F        		R1 &= ~(C_SPI_DO_Bit | C_SPI_SI_Bit | C_SPI_SCK_Bit );
00009F4C 09 A3 00 10        		R1 |= C_SPI_CS_Bit;
00009F4E 19 D3 01 20        		[P_IOA_Buffer] = R1;
                            	
                            	
00009F50 11 93 06 20        		R1 = [P_IOB_Dir];			// IOB as Input Floating 
00009F52 40 B2              		R1 &= 0x0000;
00009F53 19 D3 06 20        		[P_IOB_Dir] = R1;
00009F55 11 93 07 20        		R1 = [P_IOB_Attrib];
00009F57 09 A3 FF FF        		R1 |= 0xFFFF;
00009F59 19 D3 07 20        		[P_IOB_Attrib] = R1;
00009F5B 11 93 05 20        		R1 = [P_IOB_Buffer];
00009F5D 40 B2              		R1 &= 0x0000;
00009F5E 19 D3 05 20        		[P_IOB_Buffer] = R1;
                            		
00009F60 11 93 08 20        		R1 = [P_IO_Ctrl];
00009F62 09 A3 00 04        		R1 |= C_SPI_CS_Hardware;
00009F64 19 D3 08 20        		[P_IO_Ctrl] = R1;
                            		
00009F66 09 93 00 08        		R1 = C_SPI_Reset;		// software reset SPI
00009F68 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            	
00009F6A 09 93 02 80        		R1 = C_SPI_Enable | C_MasterMode | C_SCKSEL_FPLL_Div_8;
00009F6C 19 D3 60 20        		[P_SPI_Ctrl] = R1;
                            		
00009F6E 09 93 00 80        		R1 = C_SPI_TX_INT_Flag | C_SPI_TX_INT_DIS | C_SPI_TX_FIFO_Level_0;
00009F70 19 D3 61 20        		[P_SPI_TX_Status] = R1;
                            		
00009F72 09 93 00 80        		R1 = C_SPI_RX_INT_Flag | C_SPI_RX_INT_DIS | C_SPI_RX_FIFO_Level_1;
00009F74 19 D3 63 20        		[P_SPI_RX_Status] = R1;
                            		
00009F76 11 93 50 20        		R1 = [P_INT_Ctrl];
00009F78 19 D3 50 20        		[P_INT_Ctrl] = R1;
00009F7A 40 92              		R1 = 0x0000;
00009F7B 19 D3 52 20        		[P_FIQ_Sel] = R1;
                            		
00009F7D 90 90              		pop R1, R2 from [SP];
00009F7E 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadAWord
                            	// Description : Read a word data from flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High	
                            	// Return      : R1 = one word of data read
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_ReadAWord:	.proc
00009F7F 08 05 03 00        		R2 = SP + 3;
00009F81 D2 92              		R1 = [R2++];
00009F82 C2 94              		R2 = [R2]; 
                            		
                            	F_SPI_ReadAWord:
00009F83 98 D8              		push r2, r4 to [sp];
00009F84 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
00009F86 43 98              		R4 = C_SPI_Flash_Read_CMD;
00009F87 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009F89 0A B5 FF 00        		R2 &= 0xFF;					// Address High
00009F8B 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00009F8D 79 99              		R4 = R1 lsr 4;
00009F8E 7C 99              		R4 = R4 lsr 4;				// Address Middle
00009F8F 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009F91 09 B3 FF 00        		R1 &= 0xFF;					// Address Low
00009F93 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
00009F95 09 93 55 00        		R1 = 0x55;					// Read One Word Data dummy clock
00009F97 19 D3 62 20        		[P_SPI_TX_Data] = R1;
00009F99 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00009F9B 11 93 65 20        		R1 = [P_SPI_Misc];	
00009F9D 50 C2              		test R1, C_SPI_Busy_Flag;
00009F9E 44 4E              		jnz ?L_Check_TXIF;
00009F9F 19 D3 65 20        		[P_SPI_Misc] = R1;
                            		
                            	
00009FA1 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
00009FA3 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00009FA5 13 97 64 20        		R3 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00009FA7 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
00009FA9 12 95 64 20        		R2 = [P_SPI_RX_Data];		// Received Valid Data High from slave SPI device
00009FAB 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Received Valid Data Low from slave SPI device
00009FAD 59 93              		R1 = R1 LSL 4;
00009FAE 59 93              		R1 = R1 LSL 4;
00009FAF 02 A3              		R1 |= R2;					// Return one word data read from FLASH
                            	
00009FB0 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
00009FB2 98 92              		pop R2, R4 from [SP];
00009FB3 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_ReadNWords
                            	// Description : Get N words from external memory to buffer
                            	// Destory     : R1
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	
                            	_SPI_ReadNWords:      .proc
00009FB4 08 09 03 00        	    R4 = SP + 3;
00009FB6 D4 92              	    R1 = [R4++];							// buffer address
00009FB7 D4 94              	    R2 = [R4++];							// data length
00009FB8 D4 96              	    R3 = [R4++];							// external memory address low byte
00009FB9 C4 98              	    R4 = [R4];							// external memory address high byte
                            	
                            	F_SPI_ReadNWords:
00009FBA A8 DA              		push R1, R5 to [SP];
00009FBB 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            		
00009FBD 43 9A              		R5 = C_SPI_Flash_Read_CMD;
00009FBE 1D DB 62 20        		[P_SPI_TX_Data] = R5;
00009FC0 0C B9 FF 00        		R4 &= 0xFF;					// Address High
00009FC2 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009FC4 7B 99              		R4 = R3 lsr 4;
00009FC5 7C 99              		R4 = R4 lsr 4;				// Address Middle
00009FC6 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009FC8 0B B7 FF 00        		R3 &= 0xFF;					// Address Low
00009FCA 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            		
                            	
                            	?L_Check_TXIF:					// Wait untill command and address have been sent out
00009FCC 15 9B 65 20        		R5 = [P_SPI_Misc];	
00009FCE 50 CA              		test R5, C_SPI_Busy_Flag;
00009FCF 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
00009FD0 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Read Command transmitted   
00009FD2 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address High transmitted
00009FD4 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Middle transmitted
00009FD6 14 99 64 20        		R4 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO at Address Low transmitted
                            	
                            	?L_GetNWordLoop:
00009FD8 1B D7 62 20        		[P_SPI_TX_Data] = R3;		// Read One Word Data dummy clock
00009FDA 1B D7 62 20        		[P_SPI_TX_Data] = R3;
                            	
                            	?L_GetNWordLoop_Check_TXIF:		// Wait untill command and address have been sent out
00009FDC 15 9B 65 20        		R5 = [P_SPI_Misc];	
00009FDE 50 CA              		test R5, C_SPI_Busy_Flag;
00009FDF 44 4E              		jnz ?L_GetNWordLoop_Check_TXIF;
                            	
                            		
00009FE0 13 97 64 20        		R3 = [P_SPI_RX_Data];		// low byte data
00009FE2 14 99 64 20        		R4 = [P_SPI_RX_Data];		// high byte data
00009FE4 5C 99              		R4 = R4 lsl 4;
00009FE5 5C 99              		R4 = R4 lsl 4;
00009FE6 03 A9              		R4 |= R3;					// Return one word data read from FLASH
00009FE7 D1 D8              		[R1++] = R4;
00009FE8 41 24              		R2 -= 1;
00009FE9 52 4E              		jnz ?L_GetNWordLoop;
                            	
00009FEA 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            		
                            	?_Finish_Last:
00009FEC A8 90              		pop R1, R5 from [SP];
                            	
00009FED 90 9A              		retf;
                            	.endp
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendAWord
                            	// Description : Write a word data to flash
                            	// Destory     : R1
                            	// Parameter   : R1 = Address Low, R2 = Address High, R3 = one word of data
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_SendAWord:	.proc
00009FEE 08 07 03 00        		R3 = SP + 3;
00009FF0 D3 92              		R1 = [R3++];
00009FF1 D3 94              		R2 = [R3++];
00009FF2 C3 96              		R3 = [R3];
                            		
                            	F_SPI_SendAWord:
00009FF3 A8 DA              		push R1, R5 to [SP];
                            		
00009FF4 40 F0 73 A0        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
00009FF6 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
00009FF8 42 98              		R4 = C_SPI_Flash_Page_Program;
00009FF9 1C D9 62 20        		[P_SPI_TX_Data] = R4;
00009FFB 0A B5 FF 00        		R2 &= 0xFF;						// Address High
00009FFD 1A D5 62 20        		[P_SPI_TX_Data] = R2;
00009FFF 79 99              		R4 = R1 lsr 4;
0000A000 7C 99              		R4 = R4 lsr 4;					// Address Middle
0000A001 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000A003 09 B3 FF 00        		R1 &= 0xFF;						// Address Low
0000A005 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            		
0000A007 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data Low Byte
0000A009 7B 97              		R3 = R3 lsr 4;
0000A00A 7B 97              		R3 = R3 lsr 4;
0000A00B 1B D7 62 20        		[P_SPI_TX_Data] = R3;			// Data High Byte
                            		
                            	
                            	?L_Check_TXIF:
0000A00D 11 93 65 20        		R1 = [P_SPI_Misc];	
0000A00F 50 C2              		test R1, C_SPI_Busy_Flag;
0000A010 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000A011 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
0000A013 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A015 12 95 64 20        		R2 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A017 13 97 64 20        		R3 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A019 14 99 64 20        		R4 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A01B 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A01D 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            		
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000A01F 40 F0 96 A0        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000A021 41 C2              		test R1, C_Flash_Busy;
0000A022 44 4E              		jnz ?L_Check_Busy;
                            		
0000A023 A8 90              		pop R1, R5 from [SP];
0000A024 90 9A              		retf;	
                            		.endp
                            	
                            	
                            	//****************************************************************
                            	// Function    : F_SPI_SendNWords
                            	// Description : Send N words to external memory from internal buffer
                            	// Destory     : None
                            	// Parameter   : R1 : buffer address
                            	//               R2 : data length
                            	//               R3 : external memory address low word
                            	//               R4 : external memory address high word
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	 _SPI_SendNWords:	.proc
0000A025 08 09 04 00        		R4 = SP + 4;
0000A027 D4 92              		R1 = [R4++];
0000A028 D4 94              		R2 = [R4++];
0000A029 D4 96              		R3 = [R4++];
0000A02A C4 98              		R4 = [R4];
                            	F_SPI_SendNWords:
0000A02B A8 DA              		push R1, R5 to [SP];
                            	
                            	?L_WriteData:
                            		
0000A02C 40 F0 73 A0        		call F_Flash_Write_Enable		// Flash must be written enable firstly before it is programmed or erased.
                            	
0000A02E 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
                            	
0000A030 42 9A              		R5 = C_SPI_Flash_Page_Program;
0000A031 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000A033 1C D9 62 20        		[P_SPI_TX_Data] = R4;
0000A035 7B 9B              		R5 = R3 lsr 4;
0000A036 7D 9B              		R5 = R5 lsr 4;					// Address Middle
0000A037 1D DB 62 20        		[P_SPI_TX_Data] = R5;
0000A039 0B BB FF 00        		R5 = R3 & 0xFF;					// Address Low
0000A03B 1D DB 62 20        		[P_SPI_TX_Data] = R5;
                            		
                            	
                            	?L_Check_TXIF:
0000A03D 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000A03F 50 CA              		test R5, C_SPI_Busy_Flag;
0000A040 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000A041 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A043 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A045 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A047 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
                            	
                            	?L_SendDataLoop:
0000A049 D1 9A              		R5 = [R1++];
0000A04A 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data Low Byte
0000A04C 7D 9B              		R5 = R5 lsr 4;
0000A04D 7D 9B              		R5 = R5 lsr 4;
0000A04E 1D DB 62 20        		[P_SPI_TX_Data] = R5;			// Data High Byte
                            	
                            	?L_Check_TXIF_2:
0000A050 15 9B 65 20        		R5 = [P_SPI_Misc];	
0000A052 50 CA              		test R5, C_SPI_Busy_Flag;
0000A053 44 4E              		jnz ?L_Check_TXIF_2;
                            	
0000A054 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO 
0000A056 15 9B 64 20        		R5 = [P_SPI_RX_Data];			// Clear dummy data in RX FIFO
0000A058 41 24              		R2 -= 1;
0000A059 11 5E              		jz ?L_SendDataEnd;
0000A05A 42 06              		R3 += 2;
0000A05B 0B C7 FF 00        		test R3, 0x00FF;
0000A05D 55 4E              		jnz ?L_SendDataLoop;
                            	?L_JumpToNextPage:
0000A05E 40 46              		cmp R3, 0x0000;
0000A05F 01 4E              		jne ?L_WriteCurrentPage;
0000A060 41 08              		R4 += 1;
                            	?L_WriteCurrentPage:
0000A061 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
0000A063 88 D2              		push R1 to [SP];
                            	?L_Check_Busy_1:					// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000A064 40 F0 96 A0        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000A066 41 C2              		test R1, C_Flash_Busy;
0000A067 44 4E              		jnz ?L_Check_Busy_1;
0000A068 88 90              		pop R1 from [SP];
0000A069 0F 9F 2C A0        		pc = ?L_WriteData;
                            	
                            	?L_SendDataEnd:
0000A06B 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
                            	
                            	?L_Check_Busy:						// Wait untill one word data has been written into flash successfully (about 3 to 12ms).
0000A06D 40 F0 96 A0        		call F_SPI_Read_Status_Register; // WatchDog overflow can escape from dead loop
0000A06F 41 C2              		test R1, C_Flash_Busy;
0000A070 44 4E              		jnz ?L_Check_Busy;
                            		
                            	
0000A071 A8 90              		pop R1, R5 from [SP];
0000A072 90 9A              		retf;
                            		.endp
                            	
                            	//****************************************************************
