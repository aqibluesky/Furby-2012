0000A0A4 11 93 64 20        		R1 = [P_SPI_RX_Data];			// Received Status Register Data
                            		
0000A0A6 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
0000A0A8 90 9A              		retf;
