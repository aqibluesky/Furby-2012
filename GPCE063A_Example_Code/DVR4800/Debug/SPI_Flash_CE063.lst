0000A0D6 01 95              		R2 = R1;
0000A0D7 40 92              		R1 = 0x0000;
                            	.endif
                            	
0000A0D8 6C F2 01 20        		clrb [P_IOA_Buffer], C_SPI_CS_IO;		// enable SPI Flash
0000A0DA 0B 97 D8 00        		R3 = C_SPI_Flash_Sector_Erase;
0000A0DC 1B D7 62 20        		[P_SPI_TX_Data] = R3;
0000A0DE 1A D5 62 20        		[P_SPI_TX_Data] = R2;		// Sector index : Sector size = 64K bytes
0000A0E0 79 97              		R3 = R1 lsr 4;			
0000A0E1 7B 97              		R3 = R3 lsr 4;
0000A0E2 1B D7 62 20        		[P_SPI_TX_Data] = R3;
0000A0E4 09 B3 FF 00        		R1 &= 0x00FF;
0000A0E6 19 D3 62 20        		[P_SPI_TX_Data] = R1;
                            	
                            	
                            	?L_Check_TXIF:
0000A0E8 11 93 65 20        		R1 = [P_SPI_Misc];		// Wait untill command has been transmitted.
0000A0EA 50 C2              		test R1, C_SPI_Busy_Flag;
0000A0EB 44 4E              		jnz ?L_Check_TXIF;
                            	
                            		
0000A0EC 5C F2 01 20        		setb [P_IOA_Buffer], C_SPI_CS_IO;	// disable SPI Flash
                            	
0000A0EE 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
0000A0F0 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
0000A0F2 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
0000A0F4 11 93 64 20        		R1 = [P_SPI_RX_Data];		// Clear dummy data in RX FIFO
                            		
                            	?L_Check_Busy:					// Wait untill sector has been erased successfully (about 1 to 3 seconds)
