                            	// Parameter   : R1 = Sector Address Low, R2 = Sector Address High
                            	// Return      : None
                            	// Note        : None
                            	//****************************************************************
                            	_SPI_Flash_Sector_Erase:	.proc
                            	
0000A0D0 08 03 03 00        		R1 = SP + 3;
0000A0D2 C1 92              		R1 = [R1];
                            		
                            	F_SPI_Flash_Sector_Erase:
0000A0D3 98 D6              		push R1, R3 to [SP];
0000A0D4 40 F0 73 A0        		call F_Flash_Write_Enable;	// Enable sector erase command 
                            			
                            	.if 0		// for GPR25L005, GPR25L010, GPR25L020, GPR25L040, GPR25L080
                            		R2 = SECTOR_SIZE;
